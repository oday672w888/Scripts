att1.Position = Vector3.new(-0.5,-1,-1)--position

att1.Rotation = Vector3.new(90, 0, 0)--rotation

local AP = Instance.new("AlignPosition", i)

AP.Attachment0 = att0

AP.Attachment1 = att1

AP.RigidityEnabled = false

AP.ReactionForceEnabled = false

AP.ApplyAtCenterOfMass = true

AP.MaxForce = 9999999

AP.MaxVelocity = math.huge

AP.Responsiveness = 1000000000000

local AO = Instance.new("AlignOrientation", i)

AO.Attachment0 = att0

AO.Attachment1 = att1

AO.ReactionTorqueEnabled = true

AO.PrimaryAxisOnly = false

AO.MaxTorque = 9999999

AO.MaxAngularVelocity = math.huge

AO.Responsiveness = 10000000000000

end

Character["Left Leg"]:BreakJoints()--breaks the joints so you can customize its position

align(Character["Left Leg"], Character["Torso"]) -- the left arm is the part you want to change position, the torso part is what you want the position to be aligned to

